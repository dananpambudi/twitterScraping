{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": [
            "off",
            { "target": "single" } // default is "single"
        ],
        "import/extensions": [
            "never" ,
            {
              "js":"never"
            }
          ]
    }
}
